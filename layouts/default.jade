doctype html
html
  head
    block head
      title #{title}
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(href='/css/bootstrap.min.css', rel='stylesheet')
    link(href='/css/sb-admin.css', rel='stylesheet')
    link(href='/css/main.min.css', rel='stylesheet')
    link(href='/css/font-awesome.min.css', rel='stylesheet')
    script.
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-23794447-6', 'auto');
      ga('send', 'pageview');
    block neck
  body(style="background:white;")
    #wrapper
      // Navigation
      nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') #{projectName}
        // Top Menu Items
        ul.nav.navbar-right.top-nav
          if user && user.username    
            li
              a(href='/logout/')
                i.fa.fa-fw.fa-power-off
                |  (#{user.username})
                |  Cerrar Sesión
          else
            li
              a(href='/login/')
                i.fa.fa-user
                |  Iniciar Sesión
        if user && user.username    
          // Menu de la Izquierda
          .collapse.navbar-collapse.navbar-ex1-collapse
            ul.nav.navbar-nav.side-nav
              li
                a#in_0(href='/')
                  i.fa.fa-fw.glyphicon.glyphicon-home
                  |  Inicio  
                  li
                    a(href='/account/grafica', data-toggle='collapse', data-target='#analizar','id'='menu_analizar')
                      i.fa.glyphicon.glyphicon-star
                      |  Analizar
                      i.fa.fa-fw
                  li
                    a(href='/account/grafica/registro/', data-toggle='collapse', data-target='#registro','id'='menu_registro')
                      i.fa.glyphicon.glyphicon-star
                      |  Historial
                      i.fa.fa-fw
      div.page
        div.container
          block body

    div.footer
      div.container
        span.copyright.pull-right
          |&#169; #{copyrightYear} #{copyrightName}
        div.clearfix

    //if lte IE 9
    script(src='/layouts/core.min.js?#{cacheBreaker}')
    script(src='/layouts/ie-sucks.min.js?#{cacheBreaker}')
    script(src='/views/js/jquery.form.js')
    script(src='/views/js/jquery.min.js')

    block feet
